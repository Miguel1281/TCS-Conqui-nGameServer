<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<section name="entityFramework"
				 type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089"
				 requirePermission="false" />
	</configSections>

	<appSettings>
		<add key="aspnet:UseTaskFriendlySynchronizationContext" value="true" />
	</appSettings>

	<system.web>
		<compilation debug="true" />
	</system.web>

	<system.serviceModel>
		<services>
			<service name="ConquiánServidor.Services.SignUp">
				<host>
					<baseAddresses>
						<add baseAddress="net.tcp://localhost:9001/ConquiánServidor/SignUp/" />
					</baseAddresses>
				</host>
				<endpoint address=""
						  binding="netTcpBinding"
						  contract="ConquiánServidor.Contracts.ServiceContracts.ISignUp">
					<identity>
						<dns value="localhost" />
					</identity>
				</endpoint>
				<endpoint address="mex"
						  binding="mexTcpBinding"
						  contract="IMetadataExchange" />
			</service>

			<service name="ConquiánServidor.Services.Login">
				<host>
					<baseAddresses>
						<add baseAddress="net.tcp://localhost:9001/ConquiánServidor/Login/" />
					</baseAddresses>
				</host>
				<endpoint address=""
						  binding="netTcpBinding"
						  contract="ConquiánServidor.Contracts.ServiceContracts.ILogin">
					<identity>
						<dns value="localhost" />
					</identity>
				</endpoint>
				<endpoint address="mex"
						  binding="mexTcpBinding"
						  contract="IMetadataExchange" />
			</service>

			<service name="ConquiánServidor.Services.UserProfile">
				<host>
					<baseAddresses>
						<add baseAddress="net.tcp://localhost:9001/ConquiánServidor/UserProfile/" />
					</baseAddresses>
				</host>
				<endpoint address=""
						  binding="netTcpBinding"
						  contract="ConquiánServidor.Contracts.ServiceContracts.IUserProfile">
					<identity>
						<dns value="localhost" />
					</identity>
				</endpoint>
				<endpoint address="mex"
						  binding="mexTcpBinding"
						  contract="IMetadataExchange" />
			</service>

			<service name="ConquiánServidor.Services.FriendList">
				<host>
					<baseAddresses>
						<add baseAddress="net.tcp://localhost:9001/ConquiánServidor/FriendList/" />
					</baseAddresses>
				</host>
				<endpoint address=""
						  binding="netTcpBinding"
						  contract="ConquiánServidor.Contracts.ServiceContracts.IFriendList">
					<identity>
						<dns value="localhost" />
					</identity>
				</endpoint>
				<endpoint address="mex"
						  binding="mexTcpBinding"
						  contract="IMetadataExchange" />
			</service>

			<service name="ConquiánServidor.Services.Lobby">
				<host>
					<baseAddresses>
						<add baseAddress="net.tcp://localhost:9001/ConquiánServidor/Lobby/" />
					</baseAddresses>
				</host>
				<endpoint address=""
						  binding="netTcpBinding"
						  contract="ConquiánServidor.Contracts.ServiceContracts.ILobby">
					<identity>
						<dns value="localhost" />
					</identity>
				</endpoint>
				<endpoint address="mex"
						  binding="mexTcpBinding"
						  contract="IMetadataExchange" />
			</service>

		</services>

		<behaviors>
			<serviceBehaviors>
				<behavior>
					<serviceMetadata httpGetEnabled="false" httpsGetEnabled="false" />
					<serviceDebug includeExceptionDetailInFaults="true" />
				</behavior>
			</serviceBehaviors>
		</behaviors>
	</system.serviceModel>

	<entityFramework>
		<providers>
			<provider invariantName="System.Data.SqlClient"
					  type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
		</providers>
	</entityFramework>

	<connectionStrings>
		<add name="ConquiánDBEntities"
			 connectionString="metadata=res://*/ConquiánDB.ConquiánModelDB.csdl|res://*/ConquiánDB.ConquiánModelDB.ssdl|res://*/ConquiánDB.ConquiánModelDB.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=ASUSVIVOBOOK\SQLEXPRESS;initial catalog=ConquiánDB;user id=Admin; password=1234;trustservercertificate=True;MultipleActiveResultSets=True;App=EntityFramework&quot;"
			 providerName="System.Data.EntityClient" />
	</connectionStrings>
</configuration>